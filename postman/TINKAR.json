{
    "openapi": "3.0.1",
    "info": {
        "title": "OpenCDx TINKAR Service",
        "version": "v0"
    },
    "servers": [
        {
            "url": "https://localhost:8080/tinkar",
            "description": "Generated server url"
        }
    ],
    "security": [
        {
            "bearerAuth": []
        }
    ],
    "paths": {
        "/search": {
            "get": {
                "tags": [
                    "open-cdx-rest-tinkar-search-controller"
                ],
                "operationId": "search",
                "parameters": [
                    {
                        "name": "query",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "maxResults",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int32"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "$ref": "#/components/schemas/TinkarSearchQueryResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/descendants/conceptId": {
            "get": {
                "tags": [
                    "open-cdx-rest-tinkar-search-controller"
                ],
                "operationId": "getTinkarDescendantConcepts",
                "parameters": [
                    {
                        "name": "conceptId",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "$ref": "#/components/schemas/TinkarGetResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/conceptId": {
            "get": {
                "tags": [
                    "open-cdx-rest-tinkar-search-controller"
                ],
                "operationId": "getEntity",
                "parameters": [
                    {
                        "name": "conceptId",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "$ref": "#/components/schemas/TinkarGetResult"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/children/conceptId": {
            "get": {
                "tags": [
                    "open-cdx-rest-tinkar-search-controller"
                ],
                "operationId": "getTinkarChildConcepts",
                "parameters": [
                    {
                        "name": "conceptId",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "$ref": "#/components/schemas/TinkarGetResponse"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "TinkarSearchQueryResponse": {
                "type": "object",
                "properties": {
                    "results": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/TinkarSearchQueryResult"
                        }
                    }
                }
            },
            "TinkarSearchQueryResult": {
                "type": "object",
                "properties": {
                    "nid": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "rcNid": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "patternNid": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "fieldIndex": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "score": {
                        "type": "number",
                        "format": "float"
                    },
                    "highlightedString": {
                        "type": "string"
                    }
                }
            },
            "TinkarGetResponse": {
                "type": "object",
                "properties": {
                    "results": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/TinkarGetResult"
                        }
                    }
                }
            },
            "TinkarGetResult": {
                "type": "object",
                "properties": {
                    "conceptId": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    }
                }
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "name": "bearerAuth",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    }
}